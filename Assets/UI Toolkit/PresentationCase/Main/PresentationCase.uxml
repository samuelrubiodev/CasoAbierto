                <engine:VisualElement name="one-data" class="container-data">
                    <engine:Label text="Fecha y Tiempo" name="title-container" class="mediumTitle" />
                    <engine:Label text="Fecha del Suceso: 2024-04-15" name="date" class="small-text" />
                    <engine:Label text="Tiempo restante: 45 min" name="time_remaining" class="small-text" />
                </engine:VisualElement>
            </engine:VisualElement>
        </engine:VisualElement>
